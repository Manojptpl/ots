    function isNumberKey(evt) {
        var charCode = (evt.which) ? evt.which : event.keyCode
        if (charCode != 46 && charCode > 31 && (charCode < 48 || charCode > 57)) {
            return false;
        }
        else {
            return true;
        }
    }
    function AvgCalculate() {
        var total = 0;
        var subtotal = 0;
        var overallRating = 0;
        var Avg = 0;
        var FINAL_RATING = parseFloat($("#txt_editFinalRatingTemp").val());
        var CURRENTROLL_MGRRATING = parseInt($("#txt_editCURRENTROLL_MGRRATING").val());
        var QUALITYWORK_MGRRATING = parseInt($("#txt_editQUALITYWORK_MGRRATING").val());
        var ORGWORK_MGRRATING = parseInt($("#txt_editORGWORK_MGRRATING").val());
        var INITIATE_MGRRATING = parseInt($("#txt_editINITIATE_MGRRATING").val());
        var PERSONAL_MGRRATING = parseInt($("#txt_editPERSONAL_MGRRATING").val());
        var VSKILL_MGRRATING = parseInt($("#txt_editVSKILL_MGRRATING").val());
        var WSKILL_MGRRATING = parseInt($("#txt_editWSKILL_MGRRATING").val());
        var TEAM_MGRRATING = parseInt($("#txt_editTEAM_MGRRATING").val());
        var CONFIDENTIALITY_MGRRATING = parseInt($("#txt_editCONFIDENTIALITY_MGRRATING").val());
        var ATTENDANCE_MGRRATING = parseInt($("#txt_editATTENDANCE_MGRRATING").val());
        total = CURRENTROLL_MGRRATING + QUALITYWORK_MGRRATING + ORGWORK_MGRRATING + INITIATE_MGRRATING + PERSONAL_MGRRATING + VSKILL_MGRRATING + WSKILL_MGRRATING + TEAM_MGRRATING + CONFIDENTIALITY_MGRRATING + ATTENDANCE_MGRRATING;
        if ($.isNumeric(total)) {
            Avg = parseFloat(total / 10);
        }
        subtotal = parseFloat((FINAL_RATING + Avg));
        overallRating = parseFloat(subtotal / 2);
        $("#txt_editFinalRating").val(overallRating);
        $("#txt_editSECTION_SECOND_TOTALRATING").val(Avg);
        $("#txt_editSECTION_ONE_OVERALL_TOTALRATING").val(FINAL_RATING);
        $("#txt_editSECTION_2_OVERALL_TOTALRATING").val(Avg);
        $("#txt_editSUBTOTAL").val(subtotal);
        $("#txt_editOVERALL_RATING").val(overallRating);
    }

