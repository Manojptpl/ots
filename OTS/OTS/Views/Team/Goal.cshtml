        $(document).on("click", ".rateYo1", function (e) {
            $(this).rateYo().on("rateyo.change", function (e, data) {
                var rating = data.rating;
                var finalrating = parseFloat(parseFloat(rating) * 2);

                //alert(finalrating);
                $(this).closest('tr').find('div.indicatorContainer1').empty();
                $(this).closest('tr').find('div.indicatorContainer1').append("<input type='hidden' class='hdn_score1' value=" + finalrating + "><input type='hidden' class='hdn_rating1' value=" + finalrating + ">")
                $(this).closest('tr').find('div.indicatorContainer1').radialIndicator({
                    barColor: '#4c87c6',
                    barWidth: 40,
                    initValue: finalrating,
                    roundCorner: true,
                    percentage: false

                });
                $("canvas").css({ "width": "40px", "height": "40px" });
            });
        });
        $(document).on("click", ".rateYo", function (e) {
        $(this).rateYo().on("rateyo.change", function (e, data) {
