        $(document).ready(function () {                      
            $("#tblTeamGoal").hide();
            var minOffset = -1, maxOffset = 2;
            var thisYear = new Date().getFullYear();
            var select = $('#ddl_year');
            for (var i = 0; i <= maxOffset; i++) {
                var year = thisYear - i;
                $('<option>', { value: year, text: year }).appendTo(select);
            }                                 
            $(document).on('click', '#btnsearch', function (e) {
                debugger;
                var Quarter = $('#ddl_month option:selected').text();
                var Year = $('#ddl_year option:selected').val();
                if (Quarter == "0") {
                    alert("Please Select Quarterly.");
                    setTimeout(function () { $('#ddl_month').focus(); }, 1);
                    return false;
                }
                else if (Year == "0") {
                    alert("Please Select Year.");
                    setTimeout(function () { $('#ddl_year').focus(); }, 1);
                    return false;
                }
                GetData(Year, Quarter);                
            });            
        });
        function GetData(year, Quarter) {                     
            $.ajax({
                type: "GET",
                url: "/Team/GetTeam_Goal",
                data: { "goal_quarter": Quarter, "quarter_year": year },
                contentType: 'application/json; charset=utf-8',
                dataType: 'json',
                success: function (data) {
                    var result = JSON.parse(data);
                    if (result.length > 0) {
                        $("#tblGoal").dataTable().fnDestroy();
                        $('#tblBody').empty();
                        var html = "";
                        $.each(result, function (txt, val) {
                            html += "<tr id='info'>";  
                            html += "<td>" + val.SR_NO + "</td>";
                            html += "<td id='emp_name'>" + val.Emp_Name + "</td>";
                            html += "<td>" + val.STATUS + "</td>";
                            if (val.GOAL_STATUS != 'No Entry') {
                                html += "<td><a href='javascript:void(0);' class='tbl2' data_quarter='" + val.GOAL_QUARTER + "' data_year='" + val.QUARTER_YEAR + "' data_id='" + val.Emp_id + "' ><i class='fa fa-eye' aria-hidden='true'></a></td>";
                            }
                            else {
                                html += "<td></td>";
                            }
                            if (val.Role == '3') {
                                html += "<td></td>";
                            }
                            else {
                                html += "<td><a href='javascript:void(0);' class='tbl3' data_quarter='" + val.GOAL_QUARTER + "' data_year='" + val.QUARTER_YEAR + "' data_id='" + val.MANAGER_ID + "' ><i class='fa fa-eye' aria-hidden='true'></a></td>";
                            }
                            html += "</tr>";
                        });
                        $('#tblBody').append(html);
                        var table = $("#tblGoal").DataTable(
                            {
                                "columnDefs": [
                                    { "targets": [3, 4], "orderable": false }
                                ],
                                "language": {
                                    "search": "",
                                    "searchPlaceholder": "Search"
                                }
                            });
                    }
                    else {
                        alert("No Data Found.");
                        $('#btnExport').hide();
                    }
                },
                error: function (data) {
                    alert("Session is expired, Please login again.");
                    location.href = location.href;
                }
            });
        }
