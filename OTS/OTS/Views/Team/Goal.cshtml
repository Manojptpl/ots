            });
        });
        $(document).on("click", "#btn_update", function (e) {                                  
            $("#tbl_status tbody tr").each(function () {                
                var table = $(this);                
                var status = $(this).attr('status');                
                var data = new Array();
                var data1 = new Array();
                var data2 = new Array();                
                var strgoal = {
                    "ID": $(this).find('td').eq(0).attr('data_id'),
                    "MANAGER_REMARK":$(this).find('td').eq(8).find('input').val(),
                    "MANAGER_SCORE": $(this).find('td').eq(9).find('input').val()                    
                }
                data.push(strgoal);
                $("#tblthead tbody tr").each(function (e) {
                    var tblPersonal = $(this);
                    var strpersonal = {
                        "ID": $(this).find('td').eq(0).attr('personal_id'),
                        "MANAGER_RATING": $(this).find('td').eq(1).find('input').val(),
