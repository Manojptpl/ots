                $(this).closest('tr').find('div.indicatorContainer').empty();
                $(this).closest('tr').find('div.indicatorContainer').append("<input type='hidden' class='hdn_score' value=" + finalrating + "><input type='hidden' class='hdn_rating' value=" + finalrating + ">")
