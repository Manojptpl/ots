                        "MANAGER_REMARK": $(this).find('td').eq(3).find('input').val(),
                    }
                    data2.push(strorganizational);
                });

                var goal = { 'status': "In-Progress", 'goal_quarter': "q1", 'quarter_year': 2019 ,'goal':data,'personal':data1,'organizational':data2};
                $.ajax({
                    type: "POST",
                    url: '/Team/update_goal',
                    contentType: "application/json; charset=utf-8",
                    data: JSON.stringify(goal),
                    dataType: 'Json',
                    success: function (data) {

                        var succ = data.Success_msg;
                        var error = data.Error_msg;
                        if (succ == "SUCCESS") {
                            alert("Goal Submitted Successfully");
                        }
                        else {
                            alert(error)
                        }
                    },
                    error: function (data) {
                        alert(data.responseText);
                    }
                })
            });           
